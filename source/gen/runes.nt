import {
  this.gen.custom.runes {gen-rune-in-range},
  this.rule.gen {gen},
}

inline runes: gen(rune) {
  this.gen.custom.runes.runes(this.gen.custom.runes.Config of {
    rune-generator = none,
    rune-acceptor = {
      function (_) {
        True
      }
    },
  })
}

inline ascii-runes: gen(rune) {
  this.gen.custom.runes.runes(this.gen.custom.runes.Config of {
    rune-generator = {
      Right(function () {
        gen-rune-in-range(0x20, 0x7e)
      })
    },
    rune-acceptor = {
      function (_) {
        True
      }
    },
  })
}
