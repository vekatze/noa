import {
  this.gen {gen},
  this.gen.custom.runes {gen-rune-in-range},
  this.gen.custom.texts,
}

inline texts: gen(text) {
  this.gen.custom.texts.texts(this.gen.custom.texts.Config of {
    min-length = 0,
    max-length = none,
    rune-generator = none,
    rune-acceptor = {
      function (_) {
        True
      }
    },
  })
}

inline ascii-texts: gen(text) {
  this.gen.custom.texts.texts(this.gen.custom.texts.Config of {
    min-length = 0,
    max-length = none,
    rune-generator = {
      OK(function () {
        gen-rune-in-range(0x20, 0x7e)
      })
    },
    rune-acceptor = {
      function (_) {
        True
      }
    },
  })
}
