import {
  this.move.gen.custom.rune {gen-rune-in-range},
  this.rule.gen {gen},
}

inline rune-gen: gen(rune) {
  this.move.gen.custom.rune.rune-gen(this.move.gen.custom.rune.Config of {
    rune-generator = none,
    rune-acceptor = {
      function (_) {
        True
      }
    },
  })
}

define ascii-rune-gen: gen(rune) {
  this.move.gen.custom.rune.rune-gen(this.move.gen.custom.rune.Config of {
    rune-generator = {
      Right(function () {
        gen-rune-in-range(0x20, 0x7e)
      })
    },
    rune-acceptor = {
      function (_) {
        True
      }
    },
  })
}
